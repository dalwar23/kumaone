    "oauth_client_secret": {{ oauth_client_secret | default(None) }},
    "oauth_scopes": {{ oauth_scopes | default(None) }},
    "timeout": {{ timeout | default(60) }},
    {#- KEYWORD #}
    "keyword": {{ keyword | default(None) }},
    "invertKeyword": {{ invertKeyword | default(None) }},
    {#- GRPC_KEYWORD #}
    "grpcUrl": {{ grpcUrl | default(None) }},
    "grpcEnableTls": {{ grpcEnableTls | default(False, True) }},
    "grpcServiceName": {{ grpcServiceName | default(None) }},
    "grpcMethod": {{ grpcMethod | default(None) }},
    "grpcProtobuf": {{ grpcProtobuf | default(None) }},
    "grpcBody": {{ grpcBody | default(None) }},
    "grpcMetadata": {{ grpcMetadata | default(None) }},
