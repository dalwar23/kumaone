    "kafkaProducerBrokers": {{ kafkaProducerBrokers | default(None) }},
    "kafkaProducerTopic": {{ kafkaProducerTopic | default(None) }},
    "kafkaProducerMessage": {{ kafkaProducerMessage | default(None) }},
