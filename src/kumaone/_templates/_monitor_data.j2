{
    "type": {{ type }},
    "name": {{ name }},
    "description": {{ description | default(None) }},
    "parent": {{ parent | default(None) }},
    "interval": {{ interval | default(60) }},
    "retryInterval": {{ retryInterval | default(60) }},
    "resendInterval": {{ resendInterval | default(0) }},
    "maxretries": {{ maxretries | default(1) }},
    "notificationIDList": {{ notificationIDList | default(None) }},
    "upsideDown": {{ upsideDown | default(False, True) }},
    "httpBodyEncoding": {{ httpBodyEncoding | default("json") }},
    {#- HTTP, KEYWORD, JSON_QUERY, REAL_BROWSER #}
    "url": {{ url | default(None) }},
    {#- HTTP, KEYWORD, GRPC_KEYWORD #}
    "maxredirects": {{ maxredirects | default(10) }},
    "accepted_statuscodes": {{ accepted_statuscodes | default("200-299") }},
    {#- HTTP, KEYWORD, JSON_QUERY #}
    "expiryNotification": {{ expiryNotification | default(False, True) }},
    "ignoreTls": {{ ignoreTls | default(False, True) }},
    "proxyId": {{ proxyId | default(None) }},
    "method": {{ method | default("GET") }},
    "body": {{ body | default(None) }},
    "headers": {{ headers | default(None) }},
    "authMethod": {{ authMethod | default(None) }},
    "tlsCert": {{ tlsCert | default(None) }},
    "tlsKey": {{ tlsKey | default(None) }},
    "tlsCa": {{ tlsCa | default(None) }},
