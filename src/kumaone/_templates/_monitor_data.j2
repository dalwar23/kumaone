    "hostname": {{ hostname | default(None) }},
    {#- PING #}
    "packetSize": {{ packetSize | default(56) }},
    {#- PORT, DNS, STEAM, MQTT, RADIUS #}
    "port": {{ port | default(None) }},
    {#- DNS #}
    "dns_resolve_server": {{ dns_resolve_server | default("'1.1.1.1'") }},
    "dns_resolve_type": {{ dns_resolve_type | default("'A'") }},
