    "mqttUsername": {{ mqttUsername | default("''") }},
    "mqttPassword": {{ mqttPassword | default("''") }},
    "mqttTopic": {{ mqttTopic | default("''") }},
    "mqttSuccessMessage": {{ mqttSuccessMessage | default("''") }},
